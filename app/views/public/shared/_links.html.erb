<%- if controller_name != 'sessions' %>
  <div class="text-center">
    <%= link_to "Log in", new_session_path(resource_name), class: "btn btn_water rounded-pill w-50 m-auto my-2" %><br />
  </div>
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <div class="text-center">
    <%= link_to "Sign up", new_registration_path(resource_name), class: "btn btn_water rounded-pill w-50 m-auto my-2" %><br />
  </div>
<% end %>
